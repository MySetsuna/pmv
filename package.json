{
  "name": "pmv",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "preview:": "electron-vite preview --skipBuild",
    "start:dev": "electron-vite dev",
    "start:test": "electron-vite dev -m test",
    "start:prod": "electron-vite dev -m prod",
    "start:L1": "electron-vite dev -m L1",
    "build": "electron-vite build",
    "build:L1": "npm run typecheck && electron-vite build -m L1",
    "build:test": "npm run typecheck && electron-vite build -m test",
    "build:prod": "npm run typecheck && electron-vite build -m prod",
    "package:L1": "npm run build:L1 &&cross-env MODE=L1 electron-builder --config",
    "package:test": "npm run build:test && cross-env MODE=test electron-builder --config",
    "package:prod": "npm run build:prod && cross-env MODE=prod electron-builder --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^1.0.2",
    "electron-updater": "^5.3.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@types/node": "^18.16.19",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.0.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "electron": "^24.6.2",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.25",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "path": "^0.12.7",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.2"
  }
}
